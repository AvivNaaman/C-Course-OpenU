print_comp A
                print_comp              	           B
    	 print_comp           	     C
read_comp A,.5,3040
read_comp B,2.40000,-8.10000
read_comp C, 	   	 5.4 	 	 ,	  7.878
read_comp D             		,9,      4
read_comp E 		  ,	-5.896  ,  	 5.9435
read_comp F ,-1234,0


print_comp A
print_comp B
print_comp C
print_comp D
print_comp E
print_comp F


add_comp A                    ,B
add_comp F,E
add_comp              B ,     D

sub_comp 	 	  B 	     , 	A
sub_comp     	 E	      	,  D
sub_comp F  ,       A

mult_comp_real C        ,       0
mult_comp_real D        ,       -1
mult_comp_real F, 4.2
mult_comp_real A, -2
mult_comp_real A, -1
mult_comp_real A, -.5

mult_comp_real C,0
mult_comp_real D, -1
mult_comp_real          F       ,       4.2
mult_comp_real A, -2
mult_comp_real A, -1
mult_comp_real A        ,        -.5

mult_comp_comp A,C
mult_comp_comp B    ,   D
mult_comp_comp F,F
read_comp B,0,0
mult_comp_comp A,B
mult_comp_comp F,B
mult_comp_comp B,B

        stop

